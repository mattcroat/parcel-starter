{
  "name": "parcel-starter",
  "version": "1.0.0",
  "description": "React, TypeScript and Tailwind starter",
  "scripts": {
    "dev": "npm run build:css && parcel serve ./src/index.html -p 3000",
    "build": "npm run build:css && parcel build ./src/index.html",
    "build:css": "postcss ./src/css/tailwind.css -o ./src/css/styles.css",
    "clean": "rm -rf .parcel-cache dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "autoprefixer": "^9.8.6",
    "parcel": "^2.0.0-beta.1",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.6.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
